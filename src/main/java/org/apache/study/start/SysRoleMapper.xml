<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.apache.study.start.SysRoleMapper">
    <select id="selectSysRole" resultType="org.apache.study.start.SysRole">
        select * from sys_role where id = #{id}
    </select>
  <select id="LIST" resultType="org.apache.study.start.SysRole">
        select * from sys_role
  </select>
</mapper>
